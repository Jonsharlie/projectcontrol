{
   "name": "titulacion",
   "version": "0.0.1",
   "description": "Web application for control and monitoring of software projects",
   "type": "commonjs",
   "devDependencies": {
      "@types/cors": "^2.8.13",
      "@types/jest": "^29.5.4",
      "@types/morgan": "^1.9.4",
      "@types/node": "^16.11.10",
      "@types/node-fetch": "^2.6.4",
      "concurrently": "^8.2.0",
      "cors": "^2.8.5",
      "jest": "^29.6.4",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/express": "^4.17.17",
      "axios": "^1.5.0",
      "ejs": "^3.1.9",
      "express": "^4.18.2",
      "express-ejs-layouts": "^2.5.1",
      "morgan": "^1.10.0",
      "mysql": "^2.14.1",
      "node-fetch": "^3.3.2",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17"
   },
   "scripts": {
      "start": "npx tsc && node dist/server.js",
      "start:dev": "npx tsc && concurrently \"npx tsc -w\" \"nodemon dist/server.js\"",
      "typeorm": "npx typeorm-ts-node-esm -d ./src/config/data.source.ts",
      "m:gen": "npm run typeorm migration:generate",
